<script>
    //窗体加载事件
    $(function () {
        GetputWaAll();

    });
    //转换Data类型
    function GetTime(op) {
        var time = new Date(parseInt(op.substr(6)));
        return time.toLocaleString();
    }
    //AA表查询全部信息
    function GetputWaAll() {
        $.ajax({
            type: "POST",
            url: "/Entrepot/GetputWaAll",
            dataType: "json",
            contentType: "application/json",
            data: "{}",
            success: function (result) {
                $.each(result, function (index, data) {
                    $("#tb1").append("<tr><td><input id='Checkbox1' type='checkbox' /></td><td>" + data.receiptNum + "</td><td>" + data.receiptTypeNum + "</td><td>" + data.supplierNum + "</td><td>" + data.orderNo + "</td><td>" + data.totalNum + "</td><td>" + data.ygNumber + "</td><td>" + data.examineState + "</td><td>" + data.OperationMode + "</td><td>" + GetTime(data.createTime) + "</td><td><a href='#' onclick='return Del("+data.receiptNum +")'>删除</a></td></tr>");
                });
            },
            error: function (ex) {
                alert("Error:" + ex.responseText);
            }
        });
    }
    //删除
    function Del(receiptNum) {
        if (confirm('确定删除?')) {
            $.ajax({
                type: "POST",
                url: "/Entrepot/Del",
                data: "{receiptNum:" + receiptNum + "}",
                dataType: "json",
                contentType: "application/json",
                success: function (result) {
                    if (result > 0) {
                        alert("删除成功！");
                        location.href = '/Entrepot/Index';
                    }
                    else {
                        alert("删除失败！");
                    }
                },
                error: function (ex) {
                    alert(ex.responseText);
                }
            });
        }
        return false;
    }




</script>

